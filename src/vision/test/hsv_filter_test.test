<!-- A .test file which launches the node and its corresponding rostest -->

<launch>
    <node name="hsv_filter_node" pkg="vision" type="hsv_filter_node" />
            <param name="/hsv_filter_node/h_low" value="5" type="int" />
            <param name="/hsv_filter_node/s_low" value="230" type="int" />
            <param name="/hsv_filter_node/v_low" value="230" type="int" />
            <param name="/hsv_filter_node/h_high" value="15" type="int" />
            <param name="/hsv_filter_node/s_high" value="255" type="int" />
            <param name="/hsv_filter_node/v_high" value= "255" type="int" />
    <test test-name="hsv_filter_rostest" pkg="vision" type="hsv_filter_rostest" />
</launch>
